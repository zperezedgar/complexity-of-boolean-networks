%@article{dna,
%	author = {Nater A., Burri R., et al.},
%	title = {Resolving Evolutionary Relationships in Closely Related Species with Whole-Genome Sequencing Data},
%	journaltitle = {Systematic Biology},
%	date = {2015},
%	OPTvolume = {64(6)},
%	OPTpages = {1000-1017},

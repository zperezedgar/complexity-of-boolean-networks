@article{kolmo_automata2,
abstract = {We study the asymptotic behavior of symbolic computing systems, notably one-dimensional cellular automata (CA), in order to ascertain whether and at what rate the number of complex versus simple rules dominate the rule space for increasing neighborhood range and number of symbols (or colors), and how different behavior is distributed in the spaces of different cellular automata formalisms. Using two different measures, Shannon's block entropy and Kolmogorov complexity, the latter approximated by two different methods (lossless compressibility and block decomposition), we arrive at the same trend of larger complex behavioral fractions. We also advance a notion of asymptotic and limit behavior for individual rules, both over initial conditions and runtimes, and we provide a formalization of Wolfram's classification as a limit function in terms of Kolmogorov complexity.},
author = {Zenil, Hector and Villarreal-Zapata, Elena},
doi = {10.1142/S0218127413501599},
file = {:C$\backslash$:/Users/zpere/Dropbox/MAESTR{\'{I}}A F{\'{I}}SICA/Tesis/Art{\'{i}}culos y refs/Complexity/aplications/asymptoticbehavzenil2013.pdf:pdf},
isbn = {0218127413},
issn = {02181274},
journal = {International Journal of Bifurcation and Chaos},
keywords = {Cellular automata,Kolmogorov-Chaitin complexity,Shannon entropy,Wolfram classification,lossless compressibility},
number = {9},
pages = {1--18},
title = {{Asymptotic behavior and ratios of complexity in cellular automata}},
volume = {23},
year = {2013}
}


%@article{decomposition,
%	author = {Hector Zenil, Santiago Hernández-Orozco et al.},
%	title = {A Decomposition Method for Global Evaluation of Shannon Entropy and Local Estimations of Algorithmic Complexity},
%	journaltitle = {arXiv},
%	date = {2018},
%}
@article{decomposition,
abstract = {We investigate the properties of a Block Decomposition Method (BDM), which extends the power of a Coding Theorem Method (CTM) that approximates local estimations of algorithmic complexity based on Solomonoff–Levin's theory of algorithmic probability providing a closer connection to algorithmic complexity than previous attempts based on statistical regularities such as popular lossless compression schemes. The strategy behind BDM is to find small computer programs that produce the components of a larger, decomposed object. The set of short computer programs can then be artfully arranged in sequence so as to produce the original object. We show that the method provides efficient estimations of algorithmic complexity but that it performs like Shannon entropy when it loses accuracy. We estimate errors and study the behaviour of BDM for different boundary conditions, all of which are compared and assessed in detail. The measure may be adapted for use with more multi-dimensional objects than strings, objects such as arrays and tensors. To test the measure we demonstrate the power of CTM on low algorithmic-randomness objects that are assigned maximal entropy (e.g., $\pi$ ) but whose numerical approximations are closer to the theoretical low algorithmic-randomness expectation. We also test the measure on larger objects including dual, isomorphic and cospectral graphs for which we know that algorithmic randomness is low. We also release implementations of the methods in most major programming languages—Wolfram Language (Mathematica), Matlab, R, Perl, Python, Pascal, C++, and Haskell—and an online algorithmic complexity calculator.},
archivePrefix = {arXiv},
arxivId = {arXiv:1609.00110v7},
author = {Zenil, Hector and Hern{\'{a}}ndez-Orozco, Santiago and Kiani, Narsis A. and Soler-Toscano, Fernando and Rueda-Toicen, Antonio and Tegn{\'{e}}r, Jesper},
doi = {10.3390/e20080605},
eprint = {arXiv:1609.00110v7},
file = {:C$\backslash$:/Users/zpere/Dropbox/MAESTR{\'{I}}A F{\'{I}}SICA/Tesis/Art{\'{i}}culos y refs/Complexity/Kolmogorov Complexity/BDM Method.pdf:pdf},
issn = {10994300},
journal = {Entropy},
keywords = {Algorithmic probability,Algorithmic randomness,Information content,Information theory,Kolmogorov-Chaitin complexity,Shannon entropy,Thue-Morse sequence,$\pi$},
number = {8},
pages = {1--46},
title = {{A decomposition method for global evaluation of shannon entropy and local estimations of algorithmic complexity}},
volume = {20},
year = {2018}
}


